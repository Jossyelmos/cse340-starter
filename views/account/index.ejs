<% if (title) { %>
    <h1><%= title %></h1>
  <% } else { %>
    <h1>Oops! Something went wrong</h1>
<% } %>

<% if (messages && messages.notice) { %>
  <div class="notice">
    <%= messages.notice %>
  </div>
<% } %>

<% if (errors) { %>
  <ul class="notice">
    <% errors.array().forEach(error => { %>
      <li><%= error.msg %></li>
    <% }) %>
  </ul>
<% } %>

<h1>You're logged in</h1>